<html>
	<head>
		<title>Listagem de produtos</title>
	</head>
	<body>
		<table style="border: solid 1px">
			<th>Nome</td>
			<th>Preço</td>
			<th>Descrição</td>
			<%
			for(var i = 0; i < lista.length;i++){
			%>
				<tr>
					<td><%= lista[i].nome %></td>
					<td><%= lista[i].preco %></td>
					<td><%= lista[i].descricao %></td>
					<td>
						<a href="/produtos/remover">remover</a>
					</td>
				</tr>
			<%}%>			
		</table>
		<div>
			<a href="/produtos/novo">Novo Produto </a>
		</div>
	</body>
</html>
